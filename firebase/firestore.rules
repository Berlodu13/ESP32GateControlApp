rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /gates/{gateId} {
      allow read, write: if request.auth != null;
    }
    match /logs/{logId} {
      allow read: if request.auth != null;
      allow write: if request.auth != null && request.resource.data.timestamp is timestamp;
    }
  }
}
